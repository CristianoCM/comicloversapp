{
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 2,
    "no-var": 2,
    "vars-on-top": 1,
    "block-scoped-var": 2,
    "no-console": 1,
    "no-empty": 1,
    "no-extra-semi": 2,
    "no-extra-parens": 1,
    "no-template-curly-in-string": 2,
    "curly" : 2,
    "no-alert": 2,
    "no-empty-function": 2,
    "no-global-assign": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 1,
    "no-useless-concat": 1,
    "yoda": 1,
    "no-catch-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "block-spacing": 1,
    "brace-style": 1,
    "camelcase": 2,
    "comma-dangle": 1,
    "comma-spacing": 1,
    "func-names": 2,
    "func-call-spacing": 1,
    "indent": ["error", 2],
    "no-mixed-spaces-and-tabs": 2,
    "no-const-assign": 2,
    "prefer-arrow-callback": 2,
    "object-shorthand": 1,
    "quotes": ["warn", "single"]
  }
}
